  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
    <% @categories.each do |category|%>
      <tr>
        <td><%= category.name%></td>
      </tr>
    <% end %>
  </table>
</div>